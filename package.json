{
  "name": "mateowang.dev",
  "description": "a personal website where I post my projects, blog post, video tutorials, reading material, or whatever else I find myself doing",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SwiftWinds/mateowang.dev.git"
  },
  "author": "Mateo Wang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SwiftWinds/mateowang.dev/issues"
  },
  "homepage": "https://github.com/SwiftWinds/mateowang.dev#readme",
  "scripts": {
    "dev": "sapper dev",
    "build": "sapper build --legacy",
    "export": "sapper export --legacy",
    "start": "node __sapper__/build",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run",
    "commit": "git-cz",
    "release": "standard-version"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "date-fns": "^2.15.0",
    "gray-matter": "^4.0.2",
    "marked": "^1.1.1",
    "polka": "^0.5.2",
    "prismjs": "^1.20.0",
    "reading-time": "^1.2.0",
    "sirv": "^1.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/runtime": "^7.10.5",
    "@commitlint/cli": "^9.1.1",
    "@commitlint/config-conventional": "^9.1.1",
    "commitizen": "^4.1.2",
    "cz-conventional-changelog": "^3.2.0",
    "husky": "^4.2.5",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.22.2",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-glob": "^1.0.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-svelte": "^5.2.3",
    "rollup-plugin-terser": "^6.1.0",
    "sapper": "^0.27.16",
    "standard-version": "^8.0.2",
    "svelte": "^3.24.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
